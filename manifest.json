{
  "manifest_version": 2,
  "name": "Aware Browsing",
  "version": "1.0.0",
  "description": "An simple chrome extension that ask why you are using the browser.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "options_page": "options.html",
  "permissions": ["storage", "<all_urls>", "notifications"],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  }
}
